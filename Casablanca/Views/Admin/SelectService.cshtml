@model Casablanca.Models.ViewModel.ChangeServiceVM

@*@model List<Casablanca.Models.Service>*@

@{
    ViewBag.Title = "Admin - Select Service";
}

<h2>ChangeService</h2>

@for (int i = 0; i < Model.Services.Count(); i++)
{
    <a href="~/Admin/ChangeService?collId=@Model.Collaborator.Id&serviceId=@Model.Services[i].Id">@Html.DisplayFor(model => Model.Services[i].Name)</a>
    <br />
    }

    <div class="text-warning">@ViewBag.SelectStatus</div>

